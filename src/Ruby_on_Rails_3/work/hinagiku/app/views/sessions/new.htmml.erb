<%= form_tag :session do %>
<ul>
  <li>
    <%= label_tag :login_name, "ログイン名" %>
    <%= text_field_tag :login_name, params[:login_name] %>
  </li>
  <li>
    <%= label_tag :password, "パスワード" %>
    <%= password_field_tag :password %>
  </li>
  <li class="buttons">
   <%= submit_tag "ログイン" %>
  </li>
</ul>
<% end %>